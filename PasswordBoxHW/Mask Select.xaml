<Window x:Class="PasswordBoxHW.Mask_Select"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordBoxHW"
        mc:Ignorable="d"
        Title="Mask_Select" Height="450" Width="800">
    <Grid>
        <Button x:Name="alert"  Content="!" HorizontalAlignment="Left" Height="37" Margin="120,76,0,0" VerticalAlignment="Top" Width="70" Click="change"/>
        <Button x:Name="star" Content="*" HorizontalAlignment="Left" Height="37" Margin="339,76,0,0" VerticalAlignment="Top" Width="70" Click="change"/>
        <Button x:Name="dollar" Content="$" HorizontalAlignment="Left" Height="37" Margin="557,76,0,0" VerticalAlignment="Top" Width="70" Click="change"/>
        <Button x:Name="at"  Content="@" HorizontalAlignment="Left" Height="37" Margin="557,163,0,0" VerticalAlignment="Top" Width="70" Click="change"/>
        <Button x:Name="idk"  Content="Ѧ" HorizontalAlignment="Left" Height="37" Margin="120,163,0,0" VerticalAlignment="Top" Width="70" Click="change"/>
    </Grid>
</Window>
