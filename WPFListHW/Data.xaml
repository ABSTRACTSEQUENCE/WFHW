<Window x:Class="WPFListHW.Data"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFListHW"
        mc:Ignorable="d"
        Title="Data" Height="450" Width="800">
    <Grid>
        <Label Content="Название" HorizontalAlignment="Left" Margin="204,130,0,0" VerticalAlignment="Top" FontSize="10"/>
        <TextBox x:Name="tb_name" HorizontalAlignment="Left" Height="23" Margin="258,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" TextChanged="TextBox_TextChanged"/>
        <Label Content="Мощность двигателя" HorizontalAlignment="Left" Margin="152,158,0,0" VerticalAlignment="Top" FontSize="10" RenderTransformOrigin="0.491,2.957"/>
        <TextBox x:Name="tb_eng" HorizontalAlignment="Left" Height="23" Margin="258,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" RenderTransformOrigin="0.503,2.853" TextChanged="tb_eng_TextChanged"/>
        <Button x:Name="submit" Content="Применить" HorizontalAlignment="Left" Margin="335,186,0,0" VerticalAlignment="Top" Width="75" Click="submit_Click"/>
    </Grid>
</Window>
